        {
          "@id": "schema:Movie"
        },
        {
          "@id": "schema:CreativeWorkSeason"
        },
        {
          "@id": "schema:VideoGame"
        },
        {
          "@id": "schema:VideoGameSeries"
        },
        {
          "@id": "schema:MovieSeries"
        },
        {
          "@id": "schema:Episode"
        },
        {
          "@id": "schema:Event"
        },
        {
          "@id": "schema:VideoObject"
        },
        {
          "@id": "schema:Clip"
        },
        {
          "@id": "schema:RadioSeries"
        },
        {
          "@id": "schema:TVSeries"
        }
      ],
      "schema:rangeIncludes": {
        "@id": "schema:Person"
      }
    },
    {
      "@id": "schema:actors",
      "schema:supersededBy": {
        "@id": "schema:actor"
      }
    }
  ]
}